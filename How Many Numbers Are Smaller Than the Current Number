class Solution:
    def smallerNumbersThanCurrent(self, nums: List[int]) -> List[int]:
        count = [0] * 101
        smaller_counts = [0] * 101
        answer = [0] * len(nums)

        for num in nums:
            count[num] += 1
        
        for i in range(1, 101):
            smaller_counts[i] = smaller_counts[i-1] + count[i-1]

        for i in range(len(nums)):
            answer[i] = smaller_counts[nums[i]]

        return answer
