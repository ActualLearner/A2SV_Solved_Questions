class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        counter = [0 for _ in range(26)]

        for c in magazine:
            counter[ord('a') - ord(c)] += 1

        for c in ransomNote:
            counter[ord('a') - ord(c)] -= 1

        for i in range(26):
            if counter[i] < 0:
                return False

        return True
