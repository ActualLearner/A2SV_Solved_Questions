class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        counter = Counter(nums)
        ans = set()
        i = 0

        while len(ans) != k:
            ans.add(nums[i])
            i += 1

        for num in nums:
            for top in ans:
                if num != top and counter[num] > counter[top]:
                    ans.remove(top)
                    ans.add(num)
                    break
        
        return list(ans)
