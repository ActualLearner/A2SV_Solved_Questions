class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        set_ = set(nums)
        result = 0

        for num in set_:
            if num - 1 in set_:
                continue
            
            count = 0
            while num in set_:
                num += 1
                count += 1
            
            result = max(result, count)
    
        return result
